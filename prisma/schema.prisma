generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model projects {
  id         BigInt    @id(map: "project_pkey") @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  name       String?
}

model users {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  first_name String?
  last_name  String?
}
